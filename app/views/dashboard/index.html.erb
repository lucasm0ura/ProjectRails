                  <h3 class="box-title">DASHBOARD</h3>
                </div>
                <div class="box-body box-body-default">
                  <div class="row">
                    <% if notice.present? %>
                      <div class="alert alert-success">
                        <p id="notice">
                          <%= notice %>
                        </p>
                      </div>
                      <% end %>
                  </div>
                  <div id="accordion">
                    <ul class="list-group">
                     <a href="#demo" data-toggle="collapsed"><li class="list-group-item">Clientes Cadastrados<span class="badge"><%= @clients['TOTAL'] %></span></li></a>
                        <ul class="list-group" id="demo" class="collapse">
                          <li  class="list-group-item collapse">Pessoa Física<span class="badge"><%= @clients['CPF'] %></span></li>
                          <li  class="list-group-item collapse">Pessoa Jurídica<span class="badge"><%= @clients['CNPJ'] %></span></li>
                        </ul>
                     <li class="list-group-item">Serviços Cadastrados<span class="badge"><%= @services %></span></li> 
                     <li class="list-group-item">Orçamentos Feitos<span class="badge">23</span></li>
                      
