<div id="sb-site">
  <div class="sub-navbar">
    <div class="container-fluid">
      <div class="row">
        <div class="col-xs-12">
          <div class="sub-navbar-inner">
             <i class="sub-navbar-icon glyphicon glyphicon-home" style="font-size: 40px;"></i>            
          </div>
        </div>
      </div>
    </div>
  </div>


  <div class="content content-gray">
    <div class="container">
      <div class="row">
        <div class="box-body box-body-transparent">
          <div class="row">
            <div class="col-xs-12">
              <div class="box box-default">
